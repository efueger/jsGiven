[ignore]
.*/node_modules/json5/test/.*\.json
.*/node_modules/nodegit/node_modules/js-beautify/node_modules/config-chain/test/.*\.json
.*/flow-typed/.*
.*/dist/.*
.*/tools/.*
.*/documentation/.*
.*/examples/.*
.*/js-given/type-definitions/.*

[libs]
js-given/flow-declarations

[options]
emoji=true
esproposal.class_instance_fields=enable
esproposal.decorators=ignore
suppress_comment=\\(.\\|\n\\)*\\$FlowIgnore\\($\\|[^(]\\|(\\(>=0\\.\\(2[0-4]\\|1[0-9]\\|[0-9]\\).[0-9]\\)? *\\(site=[a-z,_]*react_native[a-z,_]*\\)?)\\)

[lints]
all=error
unclear-type=off
untyped-import=off
